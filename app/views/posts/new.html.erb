<%= render :partial => "common/user_nav" %>

<%= form_for @post, :url => admin_board_posts_path(@board, @post) do |f| %>
    <p>
        <%= f.label :subject, "Subject:" %>
        <%= f.text_field :subject %>
    </p>
    <%= f.label :content, "Content:" %>
    <p>
        <%= f.text_area :content %>
    </p>
    <%= f.submit "Create" %>
<% end %>