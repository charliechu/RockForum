<%= render :partial => "common/user_nav" %>

<ul>
<% @posts.each do |post| %>
    <li>
        <%= link_to(post.subject, admin_board_post_path(@board, post)) %>
            <%= link_to("Edit", edit_admin_board_post_path(@board, post)) %>
            <%= link_to("Destroy", admin_board_post_path(@board, post), :method => :delete )%>
    </li>
<% end %>
</ul>

<%= link_to("New Post", new_admin_board_post_path(@board)) %>